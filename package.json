{
	"name": "south-shore-technical-services-api",
	"version": "1.0.0",
	"description": "Lambda function for South Shore Technical Services",
	"main": "index.ts",
	"scripts": {
		"start": "nodemon",
		"offline": "npm run build:lambda && serverless offline",
		"build:lambda": "npm run bundle && tsc -p lambda/tsconfig.json",
		"deploy": "npm run build:lambda && serverless deploy",
		"codegen": "graphql-codegen --config codegen.yml",
		"bundle": "ncp ./src ./lambda/bundle",
		"testinvoice": "ts-node src/lib/pdfkit/index.ts"
	},
	"dependencies": {
		"@apollo/server": "^4.10.4",
		"@as-integrations/aws-lambda": "^3.1.0",
		"@aws-sdk/client-s3": "^3.583.0",
		"@aws-sdk/credential-providers": "^3.583.0",
		"@aws-sdk/s3-request-presigner": "^3.583.0",
		"@graphql-tools/schema": "^10.0.4",
		"apollo-server": "^3.13.0",
		"apollo-server-express": "^3.13.0",
		"bcryptjs": "^2.4.3",
		"dotenv": "^16.4.5",
		"graphql": "^16.8.1",
		"graphql-constraint-directive": "^5.4.3",
		"graphql-tools": "^9.0.1",
		"graphql-upload": "^16.0.2",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.6.2",
		"mongoose": "^8.1.3",
		"multer": "^1.4.5-lts.1",
		"ncp": "^2.0.0",
		"nodemailer": "^6.9.14",
		"pdfkit": "^0.15.0",
		"python-shell": "^5.0.0",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/introspection": "^4.0.2",
		"@graphql-codegen/typescript": "^4.0.4",
		"@graphql-codegen/typescript-resolvers": "^4.0.4",
		"@types/bcryptjs": "^2.4.6",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/mongoose": "^5.11.97",
		"@types/multer": "^1.4.11",
		"@types/nodemailer": "^6.4.15",
		"@types/pdfkit": "^0.13.4",
		"nodemon": "^3.0.3",
		"serverless-offline": "^13.3.3",
		"serverless-plugin-typescript": "^2.1.5",
		"ts-node": "^10.9.2"
	},
