{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"allowJs": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": ["src/*.ts", "src/**/*.ts", "src/**/*.js"],
	"exclude": ["node_modules", "src/generated/*.ts", "src/seed/*.ts", "src/**/*.d.ts"]
}
